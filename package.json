{
  "name": "org-snapshot-mirror",
  "version": "1.0.0",
  "packageManager": "pnpm@10.29.2",
  "description": "GitHub Action to mirror organization repos with snapshot commits",
  "author": "mzwing <mzwing@mzwing.eu.org>",
  "license": "MIT",
  "keywords": [
    "github-action",
    "mirror",
    "snapshot",
    "git"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --target=node24 --outfile=dist/index.js --minify --tree-shaking=true",
    "typecheck": "tsgo --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@actions/core": "^3.0.0",
    "@actions/exec": "^3.0.0",
    "@octokit/rest": "^22.0.1",
    "p-limit": "^7.3.0",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.4.3",
    "@types/node": "^25.2.3",
    "@typescript/native-preview": "7.0.0-dev.20260212.1",
    "esbuild": "^0.27.3",
    "eslint": "^9.39.2",
    "jiti": "^2.6.1",
    "typescript": "^5.9.3"
  }
}
